/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@core/*":["src/app/_core/*"],
      "@shared/*":["src/app/_shared/*"],
      "@material/*":["src/app/_material/*"],
      "@user/*":["src/app/user/*"],
      "@dashboard/*":["src/app/dashboard/*"],
      "@story/*":["src/app/story/*"],
      "@album/*":["src/app/album/*"],
      "@post/*":["src/app/post/*"],
      "@comment/*":["src/app/comment/*"],
      "@settings/*":["src/app/settings/*"],
      "@auth/*":["src/app/auth/*"],
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
